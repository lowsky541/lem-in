{
  "compilerOptions": {
    "module": "es6",
    "target": "es6",
    "moduleResolution": "node",
    "alwaysStrict": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "removeComments": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": false,
    "forceConsistentCasingInFileNames": true,
    "lib": ["es2019", "dom", "dom.iterable"]
  }
}
